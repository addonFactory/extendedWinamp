#: addon/appModules/winamp.py:127
#: addon/appModules/winamp.py:137
msgid "on"
msgstr "be"

#: addon/appModules/winamp.py:129
#: addon/appModules/winamp.py:139
msgid "off"
msgstr "ki"

#: addon/appModules/winamp.py:144
msgid "mute"
msgstr "elnémítás"

#: addon/appModules/winamp.py:145
msgid "Mute playback"
msgstr "Elnémítja a lejátszást"

#: addon/appModules/winamp.py:150
msgid "Set playback volume to 25%"
msgstr "25 %-ra állítja a lejátszási hangerőt"

#: addon/appModules/winamp.py:155
msgid "Set playback volume to 50%"
msgstr "50 %-ra állítja a lejátszási hangerőt"

#: addon/appModules/winamp.py:160
msgid "Set playback volume to 100%"
msgstr "100 %-ra állítja a lejátszási hangerőt"

#: addon/appModules/winamp.py:164
msgid "Pan left"
msgstr "Balra tolja a hangot"

#: addon/appModules/winamp.py:168
msgid "Pan right"
msgstr "Jobbra tolja a hangot"

#: addon/appModules/winamp.py:172
msgid "center"
msgstr "hang középen"

#: addon/appModules/winamp.py:173
msgid "Pan center"
msgstr "Középre állítja a hangot"

#: addon/appModules/winamp.py:178
#: addon/appModules/winamp.py:185
#: addon/appModules/winamp.py:192
msgid "No time information."
msgstr "Nincs adat."

#: addon/appModules/winamp.py:180
msgid "Speaks total track length"
msgstr "Bemondja a szám teljeshosszát"

#: addon/appModules/winamp.py:187
msgid "Speaks track elapsed time"
msgstr "Bemondja a számból már eltelt időt"

#: addon/appModules/winamp.py:194
msgid "Speaks track remaining time"
msgstr "Bemondja a számból még hátralévő időt"

#: addon/appModules/winamp.py:200
#: addon/appModules/winamp.py:220
#: addon/appModules/winamp.py:227
msgid "not playing"
msgstr "nincs lejátszás"

#: addon/appModules/winamp.py:201
msgid "Review the end of track (last 6 seconds by default)"
msgstr "Belehallgatás a szám végébe (alapértelmezésben az utolsó 6 másodpercbe)"

#: addon/appModules/winamp.py:207
#: addon/appModules/winamp.py:234
msgid "Seconds:"
msgstr "Másodperc:"

#: addon/appModules/winamp.py:207
msgid "Set review time"
msgstr "Mennyit játsszon le a szám végéből"

#: addon/appModules/winamp.py:211
#: addon/appModules/winamp.py:238
msgid "Bad value entered! Please try again."
msgstr "Hibás értéket írt be! Kérem írjon be új értéket."

#: addon/appModules/winamp.py:211
#: addon/appModules/winamp.py:238
msgid "Error"
msgstr "Hiba"

#: addon/appModules/winamp.py:214
msgid "Set the review time (in seconds) for use with Review End of Track command"
msgstr "Ezzel a paranccsal megadhatja, hogy hány másodpercet játsszon le a szám végéből a \"belehallgatás a szám végébe\" parancs"

#: addon/appModules/winamp.py:221
msgid "Alternate jump forward (6 seconds by default)"
msgstr "Szabályozható előretekerés (alapértelmezésben 6 másodperccel)"

#: addon/appModules/winamp.py:228
msgid "Alternate jump backward (6 seconds by default)"
msgstr "Szabályozható hátratekerés (alapértelmezésben 6 másodperccel)"

#: addon/appModules/winamp.py:234
msgid "Set alternate jump time"
msgstr "Mennyit tekerjen szabályozható tekerésnél"

#: addon/appModules/winamp.py:242
msgid "Set alternate jump time (in seconds)"
msgstr "Ezzel a paranccsal megadhatja, hogy szabályozható tekerésnél hány másodpercet tekerjen előre, vagy hátra"

